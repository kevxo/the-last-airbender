<h1>Members: there are <%= @members.count %> total</h1>

<% @members.each do |member| %>
 <ul class="member">
    <li class="name">Name <%= member.name %></li>
    <li class="photo">Photo <%= member.photo %></li>
    <li class="allies"> Alias <%= member.allies %></li>
    <li class="affiliation">Affiliation <%= member.affiliation %></li>
  </ul>
<% end %>